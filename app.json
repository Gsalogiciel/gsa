{
  "expo": {
    "name": "Gestion AUMB",
    "slug": "gestion-aumb",
    "version": "1.4.0",
    "sdkVersion": "52.0.0",
    "orientation": "portrait",
    "icon": "./assets/dd2.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Cette application nécessite l'accès à la caméra pour capturer des photos.",
        "NSPhotoLibraryUsageDescription": "Cette application nécessite l'accès à la bibliothèque pour sélectionner des images."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "gsaAumb.app",
      "versionCode": 2,
      "buildNumber": "1.2.0",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "enableProguardInReleaseBuilds": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "fca935f0-a748-4e26-bcb7-438a3c89f162"
      }
    }
  }
}
